<%= simple_form_for(@mente) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :image %>
    <%= f.input :email %>
    <%= f.input :name %>
    <%= f.input :age %>
    <%= f.input :mentor_type %>
    <%= f.input :description %>
    <%= f.input :question %>
    <%= f.input :interests %>
    <%= f.input :location %>
    <%= f.input :langitude %>
    <%= f.input :latitude %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
